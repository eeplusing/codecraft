package com.eplusing.codecraft.ref2;

public class Target
{
	private int sourceID;
	private int destinationID;
	private int[] includingID;
	public Target(int sourceID, int destinationID, int[] includingID)
	{
		super();
		this.sourceID = sourceID;
		this.destinationID = destinationID;
		this.includingID = includingID;
	}
	public int getSourceID()
	{
		return sourceID;
	}
	public void setSourceID(int sourceID)
	{
		this.sourceID = sourceID;
	}
	public int getDestinationID()
	{
		return destinationID;
	}
	public void setDestinationID(int destinationID)
	{
		this.destinationID = destinationID;
	}
	public int[] getIncludingID()
	{
		return includingID;
	}
	public void setIncludingID(int[] includingID)
	{
		this.includingID = includingID;
	}
}
